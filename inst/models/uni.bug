model{
  
  # Model structure

  for(j in 1:n_traits){
    for(i in 1:n_obvs){
      obvs[i,j] ~ dnorm(mu_trait[j], tau_trait[j])
    }
  }
  
  # Priors
  
  for(j in 1:n_traits){
    mu_trait[j] ~ dnorm(0, 0.001)
    tau_trait[j] ~ dgamma(gamma.shape, gamma.rate) 
    sigma2_trait[j] <- 1/tau_trait[j]
  }

  
}
